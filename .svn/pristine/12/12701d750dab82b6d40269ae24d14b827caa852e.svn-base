img{
	display: block;
	width:100%;
}
ul,ol,li,dl,dd,dt{list-style: none;}
*{
	padding: 0;
	margin: 0;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}
body,html,#app{
	font-size:12px;
	padding: 0;
  margin: 0;
  height:100%;
}
