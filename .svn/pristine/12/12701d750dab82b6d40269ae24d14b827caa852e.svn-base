header{
	border-bottom: solid #dedfe0 1px;
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: .88rem;
	line-height: .88rem;
    z-index:999;
}
.back{
	position: absolute;
	top: 0;
	left: .3rem;
	background:url(../images/common/ykxz_fanhui@2x.png) 0 center no-repeat;
	-webkit-background-size: .17rem .3rem;
	background-size: .17rem .3rem;
	height: .88rem;
	width: 1rem;
	line-height:.92rem;
	padding-left: .3rem;
	text-align: center;
}
.back1{
	background:url(../images/common/ykxz_fanhui@2x1.png) 0 center no-repeat;
	-webkit-background-size: .17rem .3rem;
