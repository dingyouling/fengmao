a{
	text-decoration:none;
	color: #333;
}
h1,h2,h3,h4,h5,h6{
	font-weight: normal;
}
.clearfix:after{
	visibility: hidden;
	display: block;
	font-size: 0;
	content: " ";
	clear: both;
	height: 0;
	zoom:1;
}
input,select{
	border: 0;
	outline-style: none;
	appearance:none;
	-moz-appearance:none;
	-webkit-appearance:none;
}
