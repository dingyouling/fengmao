}
.sel{
	position: absolute;
	top:0;
	right:.3rem;
	height:.88rem;
	line-height: .88rem;
}
.button{
    height:.9rem;
    line-height: .9rem;
    width:100%;
    border:none;
    outline:none;
    border-radius: 5px;
